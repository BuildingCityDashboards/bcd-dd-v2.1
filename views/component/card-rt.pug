mixin card-rt(href, style, Header, title, Chartid)
  a(href=href||null class="card__link")
    div(class=style)
      .card__header=Header
      .card__body(id=Chartid width="205" height="70")
          //- .container-fluid
          //- .row
          #card-left.card-left.col-4
          #card-center.card-center.col-4
          #card-right.card-right.col-4
                        
                    
