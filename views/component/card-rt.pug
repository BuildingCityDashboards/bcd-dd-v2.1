mixin card-rt(href, style, Header, title, Chartid)
  a(href=href||null class="card__link")
    div(class=style)
      .card__header=Header
      .card__body(id=Chartid width="250" height="70")
          #card-left.card-left
          #card-center.card-center
          #card-right.card-right
                        
                    
