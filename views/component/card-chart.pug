mixin card-chart(href, style, Header, title, Chartid, img)
  a(href=href class="card__link")
    div(class=style)
      .card__header=Header
        img(height='15px' width='15px' class="float-right" title="This is a static historical dataset" src=img)
      .card__body.card-graph
        div(id=Chartid class="line-chart" width="205" height="70")